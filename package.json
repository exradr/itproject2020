{
  "name": "camel_case_itproject",
  "version": "1.0.0",
  "description": "ePortfolio App for COMP30022 IT Project",
  "cacheDirectories": [
    "server/node_modules",
    "client/node_modules"
  ],
  "scripts": {
    "start": "cd server/ && npm start",
    "start-client": "cd client/ && npm start",
    "dev": "concurrently \"cd server/ && npm start\" \"cd client/ && npm start\"",
    "build": "concurrently \"cd client/ && npm ci && npm run build\" \"cd server && npm install\"",
    "test": "concurrently \"cd client/ && npm test\" \"cd server/ && npm test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exradr/itproject2020.git"
  },
  "keywords": [
    "react",
    "node.js",
    "express"
  ],
  "author": "Tuan Dung Nguyen, Lawrence Leong, Liam Saliba, Chan Jie Ho & Yung Cheng Kong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/exradr/itproject2020/issues"
  },
  "homepage": "https://github.com/exradr/itproject2020#readme",
  "dependencies": {
    "@styled-icons/fa-solid": "^10.18.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.23.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.7",
    "mongoose-type-email": "^1.0.12",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.4.11",
    "passport": "^0.4.1",
    "passport-facebook-token": "^4.0.0",
    "passport-google-plus-token": "^2.1.0"
  },
  "prettier": {
    "arrowParens": "avoid"
  }
}
