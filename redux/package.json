{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "description": "Redux Test Project",
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@types/redux-logger": "^3.0.8",
    "axios": "^0.20.0",
    "moment": "^2.27.0",
    "prettier": "^2.1.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6"
  },
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js",
    "test": "jest --watchAll",
    "coverage": "jest --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
